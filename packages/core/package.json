{
  "name": "@morphorm/core",
  "version": "0.1.0",
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "private": true,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.mts",
        "default": "./dist/types.mjs"
      }
    },
    "./schema": {
      "import": {
        "types": "./dist/schema/schema.d.mts",
        "default": "./dist/schema/schema.mjs"
      }
    },
    "./zod": {
      "import": {
        "types": "./dist/schema/zod/index.d.mts",
        "default": "./dist/schema/zod/index.mjs"
      }
    },
    "./zod/v4": {
      "import": {
        "types": "./dist/schema/zod/v4/index.d.mts",
        "default": "./dist/schema/zod/v4/index.mjs"
      }
    },
    "./valibot": {
      "import": {
        "types": "./dist/schema/valibot/index.d.mts",
        "default": "./dist/schema/valibot/index.mjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "check-types": "tsc --noEmit",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsdown": "^0.21.0-beta.2",
    "typescript": "^5"
  },
  "peerDependencies": {
    "zod": "^4.3.6"
  }
}
